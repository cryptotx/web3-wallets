{
  "name": "web3-wallets",
  "version": "2.1.6",
  "description": "Javascript SDK for Web3 wallets",
  "main": "lib/index.js",
  "type": "lib/index.d.ts",
  "author": "Element Project Developers",
  "scripts": {
    "release": "tsc && npm publish && git tag $npm_package_version && git push --tag",
    "dev:tsc": "tsc -w -p tsconfig.json",
    "eslint": "eslint --fix src --ext .ts --max-warnings=0",
    "start": "tsc --watch",
    "link": "sudo npm link web3-wallets"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "@coinbase/wallet-sdk": "^3.3.0",
    "@ethersproject/hash": "^5.6.1",
    "bignumber.js": "^9.0.1",
    "ethers": "^5.6.9",
    "https-proxy-agent": "^5.0.1",
    "web3-assert": "^1.0.1",
    "web3-signer-provider": "^1.0.8"
  },
  "devDependencies": {
    "https-browserify": "^1.0.0",
    "net": "^1.0.2",
    "os-browserify": "^0.3.0",
    "stream-http": "^3.2.0",
    "tls": "^0.0.1",
    "url": "^0.11.0",
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "assert": "^2.0.0",
    "eslint": "^7.32.0",
    "ethereumjs-util": "^7.1.4",
    "ts-node": "~10.7.0",
    "typescript": "~4.6.4",
    "web3": "^1.7.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "watch": "tsc --watch"
}
